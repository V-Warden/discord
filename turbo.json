{
	"$schema": "https://turborepo.org/schema.json",
	"globalDependencies": [".env.local"],
	"ui": "tui",
	"globalEnv": ["DISCORD_TOKEN", "OWNERS", "DATABASE_URL"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["build/**"]
		},
		"dev": {
			"dependsOn": [],
			"cache": false
		},
		"db:check": {
			"cache": false
		},
		"db:generate": {
			"cache": false
		},
		"db:migrate": {
			"cache": false
		},
		"db:wipe": {
			"cache": false
		},
		"test": {
			"cache": false
		}
	}
}
